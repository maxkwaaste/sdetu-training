package basics;

public class  FIbonnaci {

	public static void main(String[] args){
	
		System.out.println(fac(7));
		
	}
	
	/* 1! = 1
	 * 2! = 2 * 1
	 * 3! = 3 * 2 * 1
	 * 4! = 4 * 3 * 2 * 1
	 */
	
	public static int fac(int n)  {
		if (n == 0)  {
			return 0;
		}
		else if (n == 1) {
			return 1;
			
		}
		return (fac(n) * fac(n)-n);
	}
	
	
}
