package oop;

public class demo {

	public static void main(String[] args)	{
		//Person-
		
		//attributes, variables, descriptors
		String name = "Joe";
		String email = "Joe@testemail.com";
		String phone = "0619899851";
		
		// Activity, action, behavior
		//System.out.println(name + " is walking");
		walking(name);
		System.out.println(name + " is eating");
		
		String name2 = "Sara";
		String email2 = "sara@testemail.com";
		String phone2 = "0618899851";
		
		//what if we wanted to do this for another person
		//System.out.println(name2 + " is walking");
		walking(name2);
		System.out.println(name2 + " is eating");
			
	}
		
	//enhance by adding function, minimizing code
	
	static void walking(String name) { 
		System.out.println(name + " is walking");
	}
	
	
	
}
